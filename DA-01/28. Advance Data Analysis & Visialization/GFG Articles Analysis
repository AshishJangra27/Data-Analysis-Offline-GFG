      "source": [
        "##### 2.2) Split dates based on day, month and year"
      ],
      "metadata": {
        "id": "zG7J5x6hHz9Q"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "date   = []\n",
        "month = []\n",
        "year  = []\n",
        "\n",
        "for i in df['last_updated']:\n",
        "  i = i.replace(',','')\n",
        "  date.append(int(i.split(' ')[0]))\n",
        "  month.append(i.split(' ')[1])\n",
        "  year.append(int(i.split(' ')[2]))\n",
        "\n",
        "df['date']   = date\n",
        "df['month'] = month\n",
        "df['year']  = year\n",
        "df.head()"
      ],
