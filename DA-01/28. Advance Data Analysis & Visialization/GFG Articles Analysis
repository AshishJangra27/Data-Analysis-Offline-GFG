        "# prompt: code to find the day on a specific date\n",
        "\n",
        "import datetime\n",
        "\n",
        "def find_day(date):\n",
        "  date_obj = datetime.datetime.strptime(date, '%d %b %Y')\n",
        "  day = date_obj.strftime('%A')\n",
        "  return day\n",
        "\n",
